{
  "name": "react-seo-starter",
  "version": "1.0.0",
  "description": "Starter project for React taking into account SEO needs from the start.",
  "main": "server/bin/server.js",
  "scripts": {
    "dev": "webpack-dev-server --config webpack/client.dev.js",
    "build": "npm run build:client && npm run build:server",
    "build:server": "cross-env NODE_ENV=production webpack --config webpack/server.prod.js --progress",
    "build:client": "cross-env NODE_ENV=production webpack --config webpack/client.prod.js --progress",
    "start": "cd server && node bin/server.js",
    "lint": "eslint --ext .jsx,.js ./",
    "page:gen": "scaffold.sh",
    "test": "jest",
    "test:watch": "jest --watch",
    "coverage": "jest --coverage"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dornellaskj/SEOstarter"
  },
  "keywords": [
    "react",
    "express",
    "webpack",
    "SAAS",
    "babel",
    "expressjs",
    "seo"
  ],
  "author": "Kevin Dornellas",
  "license": "MIT",
  "homepage": "https://www.kevindornellas.com",
  "dependencies": {
    "cross-env": "^5.1.6",
    "express": "^4.16.3",
    "prop-types": "^15.6.1",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-hot-loader": "^3.1.3",
    "sitemap": "^1.13.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-1": "^6.5.0",
    "css-loader": "^1.0.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.4",
    "eslint": "^5.2.0",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.8.2",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^2.30.1",
    "jest": "^23.4.1",
    "node-sass": "^4.9.0",
    "sass-loader": "^6.0.7",
    "style-loader": "^0.18.2",
    "uglifyjs-webpack-plugin": "^0.4.6",
    "webpack": "^3.12.0",
    "webpack-dev-server": "^2.11.2",
    "webpack-node-externals": "^1.7.2"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "<rootDir>__tests__/setup/setupEnzyme.js",
    "testPathIgnorePatterns": [
      "<rootDir>/__tests__/setup/"
    ]
  },
  "bundledDependencies": [
    "cross-env",
    "ejs",
    "express",
    "prop-types",
    "react",
    "react-dom"
  ]
}
